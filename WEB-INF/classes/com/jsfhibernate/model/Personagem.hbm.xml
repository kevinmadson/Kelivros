<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jsfhibernate.model.Personagem" table="personagem" catalog="jsfhibercrud" optimistic-lock="version">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="increment"/> 
        </id>
        <property name="nome" type="string">
            <column name="Nome" />
        </property>
        <property name="livro" type="string">
            <column name="Livro" />
        </property>
        <property name="autor" type="string">
            <column name="Autor" />
        </property>
		<property name="dinheiro" type="java.lang.Float">
            <column name="Dinheiro" />
        </property>
    </class>
</hibernate-mapping>